body {
	font-family: 'Bosch Sans Light', 'Bosch Sans Regular', 'Bosch Sans' !important;
	font-size: 14px;
	line-height: 1.42857143;
	color: #333;
}

.ms-core-deltaSuiteLinks {
	display: none;
}

.ms-tableCell {
	display: inline-block !important;
}

#pageTitle {
	font-family: 'Bosch Sans Light', 'Bosch Sans Regular', 'Bosch Sans' !important;
	text-align: left !important;
	font-size: 28px;
	margin: -97px 0px 0px 445px;
	width: 60% !important;
}

#pageTitle #DeltaPlaceHolderPageTitleInTitleArea span {
	display: none;
}

.ms-breadcrumb-box {
	border-bottom: none;
	height: 38px;
	width: 75% !important;
}

/* .ms-breadcrumb-box+div {
	width: 24% !important; 
	/* 35941 Searchbox on the top right must not be visible */
	/* display: none !important;
} */

.ms-helperText,
input.ms-helperText {
	font-family: 'Bosch Sans Light' !important;
}

.ms-searchCenter-main {
	margin: 300px auto auto auto;
}

.ms-core-navigation {
	font-family: 'Bosch Sans Light', 'Bosch Sans Regular', 'Bosch Sans' !important;
}

.menu-item-text {
	color: black;
	font-weight: normal !important;
	font-size: 16px;
}

.ms-core-listMenu-selected:link,
.ms-core-listMenu-selected {
	border-bottom: 2px solid rgb(0, 86, 145) !important;
}

#DeltaTopNavigation {
	margin: 0px 0px 0px 220px;
}

#DeltaTopNavigation li a {
	padding: 2px 0px 11px 0px !important
}

#DeltaTopNavigation>.ms-listMenu-editLink,
.ms-navedit-editArea {
	display: none !important;
}

.ms-core-tableNoSpace {
	border-spacing: 0px;
	width: 100%;
}

#titleAreaBox {
	margin: auto;
}

#siteIcon {
	min-width: 64px;
	height: 40px;
	line-height: 64px;
	text-align: center;
	margin-right: 250px;
	float: left;
	margin-left: 220px;
	margin-bottom: 20px;
}

.ms-siteicon-img {
	display: block;
	max-width: 180px;
	max-height: 64px;
	z-index: 1;
	position: relative;
}

#searchInputBox .ms-srch-sb-border {
	border: none;
	border-bottom: 1px solid #ababab;
}

/* custom suite bar (page header) */

.ms-core-brandingText {
	display: none;
}

#suiteBar {
	display: block;
	height: 15px;
}

#suiteBarLeft,
#suiteBarRight {
	display: block;
}

#suiteBarLeft {
	background-image: url(data:image/png;base64,);
	background-size: cover;
	height: 15px;
}

#suiteBarRight {
	background-color: #fff;
	color: #666;
	white-space: nowrap;
	padding-right: 5px;
	float: right;
}

#ctl00_site_share_button,
#site_follow_button {
	display: none !important;
}

#globalNavBox {
	background-color: #fff !important;
	width: 100%;
	height: 35px;
}

#s4-ribbonrow {
	position: static !important;
}

#RibbonContainer-TabRowRight {
	margin-top: -5px;
	margin-right: 10px;
}

/* Hide Site Actions gear icon */
.ms-siteactions-imgspan {
	display: none;
}

/* Insert new Site Actions image */
.ms-siteactions-normal>a:before {
	content: "";
	/* Required for pseudo element */
	background: url("../Style%20Library/Images/Bosch-IC-settings.svg");
	/* Image URL */
	height: 16px;
	/* Height of image */
	width: 16px;
	/* Width of image */
	display: inline-block;
}

#ms-help>a>span img {
	display: none !important;
}

#ms-help>a>span span {
	background: url("../Style%20Library/Images/Bosch-IC-question.svg");
	/* Image URL */
	display: static !important;
	overflow: static !important;
	height: static !important;
	width: static !important;
	padding-left: 7px !important;
	padding-top: 7px !important;
	padding-right: 0px !important;
	padding-bottom: 0px !important;
	height: 12px !important;
}

#ctl00_PlaceHolderMain_diidSiteSettings>span img {
	display: none !important;
}

#ctl00_PlaceHolderMain_diidSiteSettings>span:first-child {
	background: url("../Style%20Library/Images/Bosch-IC-settings.svg");
	display: contents;
	height: 15px !important;
}

#ctl00_PlaceHolderMain_diidRecycleBin>span img {
	display: none !important;
}

#ctl00_PlaceHolderMain_diidRecycleBin>span:first-child {
	background: url("../Style%20Library/Images/Bosch-IC-delete.svg");
	display: contents;
	height: 15px !important;
}



/*  Start: Css for Master Page Footer  */

.wrapper {
	min-height: 100%;
	height: auto !important;
	height: 100%;
	/* The bottom margin is the negative value of the footer's height */
	margin: 0 auto -30px;
	overflow: visible !important;
}

.footer,
.push {
	/* .push must be the same height as .footer */
	height: 30px;
	/* Multicolumn Layout With Sticky Footer */
	clear: both;
}

.footer {
	background: #eee;
	color: black !important;
	line-height: 30px !important;
	font-family: "Lato", "Helvetica Neue", Helvetica, Arial, sans-serif !important;
	font-size: 14px !important;
	;
	padding-left: 152px !important;
	border-top: 1px solid #666 !important;
	/*				padding-left: 220px; */
}

body #s4-workspace {
	margin-bottom: -30px;
}
#s4-workspace {
    overflow: auto;
    position: relative;
}

/*  End: Css for Master Page Footer  */